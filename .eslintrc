{
    "parser": "babel-eslint",
    "rules": {
        "strict": 1,
        "no-underscore-dangle": 0,
        "indent": [
            2,
            4
        ],
        "quotes": [
            2,
            "double"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ]
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true,
        "mocha": true
    },
    "ecmaFeatures": {
        "jsx": true
    },
    "plugins": [

    ]
}
